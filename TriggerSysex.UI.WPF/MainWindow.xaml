        Title="MainWindow" Height="513.911" Width="906.634">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="885*"/>
            <ColumnDefinition Width="14*"/>
        </Grid.ColumnDefinitions>
        <TabControl x:Name="tabcMain" HorizontalAlignment="Left" Height="418" Margin="10,10,0,0" VerticalAlignment="Top" Width="875" SelectionChanged="tabMain_SelectionChanged">
            <TabItem x:Name="tabiPrograms" Header="Programs">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem x:Name="tabiTriggers" Header="Triggers">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
